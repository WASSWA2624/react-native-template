var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _react = _interopRequireDefault(require("react"));
var _react2 = require("@testing-library/react");
var _styledComponents = require("styled-components");
var _light = _interopRequireDefault(require("../../../theme/light.theme"));
var _types = require("../../../platform/components/display/Badge/types");
var _jsxRuntime = require("react/jsx-runtime");
var BadgeWeb = require("../../../platform/components/display/Badge/Badge.web").default;
var renderWithTheme = function renderWithTheme(component) {
  return (0, _react2.render)((0, _jsxRuntime.jsx)(_styledComponents.ThemeProvider, {
    theme: _light.default,
    children: component
  }));
};
describe('Badge Component - Web', function () {
  describe('Web accessibility', function () {
    it('should render with role="status"', function () {
      var _renderWithTheme = renderWithTheme((0, _jsxRuntime.jsx)(BadgeWeb, {
          variant: _types.VARIANTS.WARNING,
          testID: "web-badge",
          children: "!"
        })),
        getByRole = _renderWithTheme.getByRole;
      var badge = getByRole('status');
      expect(badge).toBeTruthy();
    });
    it('should accept className prop', function () {
      var _renderWithTheme2 = renderWithTheme((0, _jsxRuntime.jsx)(BadgeWeb, {
          className: "custom-class",
          testID: "web-badge",
          children: "Test"
        })),
        getByRole = _renderWithTheme2.getByRole;
      var badge = getByRole('status');
      expect(badge).toBeTruthy();
      expect(badge.className).toContain('custom-class');
    });
  });
  describe('Rendering', function () {
    it('should render primary variant', function () {
      var _renderWithTheme3 = renderWithTheme((0, _jsxRuntime.jsx)(BadgeWeb, {
          variant: _types.VARIANTS.PRIMARY,
          children: "5"
        })),
        getByText = _renderWithTheme3.getByText;
      expect(getByText('5')).toBeTruthy();
    });
    it('should render small size', function () {
      var _renderWithTheme4 = renderWithTheme((0, _jsxRuntime.jsx)(BadgeWeb, {
          size: _types.SIZES.SMALL,
          children: "S"
        })),
        getByText = _renderWithTheme4.getByText;
      expect(getByText('S')).toBeTruthy();
    });
  });
