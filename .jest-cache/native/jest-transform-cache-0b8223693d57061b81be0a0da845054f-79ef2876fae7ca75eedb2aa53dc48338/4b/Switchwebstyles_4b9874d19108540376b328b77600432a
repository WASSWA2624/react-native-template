var shadowToBoxShadow = function shadowToBoxShadow(shadow) {
  if (!shadow) return 'none';
  var x = shadow.shadowOffset && typeof shadow.shadowOffset.width === 'number' ? shadow.shadowOffset.width : 0;
  var y = shadow.shadowOffset && typeof shadow.shadowOffset.height === 'number' ? shadow.shadowOffset.height : 0;
  var blur = typeof shadow.shadowRadius === 'number' ? shadow.shadowRadius : 0;
  var alpha = typeof shadow.shadowOpacity === 'number' ? shadow.shadowOpacity : 0;
  var shadowColor = shadow.shadowColor || '#000000';
  if (shadowColor === '#000' || shadowColor === '#000000' || shadowColor.toLowerCase() === 'black') {
    return `${x}px ${y}px ${blur}px rgba(0, 0, 0, ${alpha})`;
  }
  return `${x}px ${y}px ${blur}px ${shadow.shadowColor || 'rgba(0, 0, 0, 0)'}`;
};
