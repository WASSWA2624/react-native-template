var _conversation = require("../../../features/conversation");
var _crudAssertions = require("../../helpers/crud-assertions");
describe('conversation.rules', function () {
  it('parses ids', function () {
    (0, _crudAssertions.expectIdParser)(_conversation.parseConversationId);
    (0, _crudAssertions.expectIdParser)(_conversation.parseConversationParticipantId);
  });
  it('parses payloads', function () {
    (0, _crudAssertions.expectPayloadParser)(_conversation.parseConversationPayload);
    (0, _crudAssertions.expectPayloadParser)(_conversation.parseConversationParticipantPayload);
    (0, _crudAssertions.expectPayloadParser)(_conversation.parseConversationMessagePayload);
  });
  it('parses list params', function () {
    (0, _crudAssertions.expectListParamsParser)(_conversation.parseConversationListParams);
  });
});
