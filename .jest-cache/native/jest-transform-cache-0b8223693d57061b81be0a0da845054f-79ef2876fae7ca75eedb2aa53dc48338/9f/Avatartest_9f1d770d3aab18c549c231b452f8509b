  describe('useAvatar Hook - Direct Testing', function () {
    var useAvatarModule = require("../../../platform/components/display/Avatar/useAvatar");
    var useAvatar = useAvatarModule.default || useAvatarModule;
    it('should use default size when size is undefined', function () {
      var _renderHook = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe',
            size: undefined
          }
        }),
        result = _renderHook.result;
      expect(result.current.size).toBe(_Avatar.SIZES.MEDIUM);
    });
    it('should use provided size', function () {
      var _renderHook2 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe',
            size: _Avatar.SIZES.LARGE
          }
        }),
        result = _renderHook2.result;
      expect(result.current.size).toBe(_Avatar.SIZES.LARGE);
    });
    it('should resolve label from accessibilityLabel', function () {
      var _renderHook3 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe',
            accessibilityLabel: 'User avatar'
          }
        }),
        result = _renderHook3.result;
      expect(result.current.label).toBe('User avatar');
    });
    it('should resolve label from alt when accessibilityLabel not provided', function () {
      var _renderHook4 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe',
            alt: 'Profile picture'
          }
        }),
        result = _renderHook4.result;
      expect(result.current.label).toBe('Profile picture');
    });
    it('should resolve label from name when neither accessibilityLabel nor alt provided', function () {
      var _renderHook5 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe'
          }
        }),
        result = _renderHook5.result;
      expect(result.current.label).toBe('John Doe');
    });
    it('should return undefined label when no name, alt, or accessibilityLabel', function () {
      var _renderHook6 = renderHook(useAvatar, {
          initialProps: {}
        }),
        result = _renderHook6.result;
      expect(result.current.label).toBeUndefined();
    });
    it('should handle string source', function () {
      var _renderHook7 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe',
            source: 'https://example.com/avatar.png'
          }
        }),
        result = _renderHook7.result;
      expect(result.current.hasImage).toBe(true);
      expect(result.current.imageSource).toEqual({
        uri: 'https://example.com/avatar.png'
      });
    });
    it('should handle object source', function () {
      var sourceObj = {
        uri: 'https://example.com/avatar.png'
      };
      var _renderHook8 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe',
            source: sourceObj
          }
        }),
        result = _renderHook8.result;
      expect(result.current.hasImage).toBe(true);
      expect(result.current.imageSource).toBe(sourceObj);
    });
    it('should set hasImage to false when no source', function () {
      var _renderHook9 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe'
          }
        }),
        result = _renderHook9.result;
      expect(result.current.hasImage).toBe(false);
    });
    it('should set hasImage to false when image error occurs', function () {
      var _renderHook0 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe',
            source: 'https://example.com/avatar.png'
          }
        }),
        result = _renderHook0.result;
      expect(result.current.hasImage).toBe(true);
      act(function () {
        result.current.handleImageError();
      });
      expect(result.current.hasImage).toBe(false);
    });
    it('should generate initials from name', function () {
      var _renderHook1 = renderHook(useAvatar, {
          initialProps: {
            name: 'John Doe'
          }
        }),
        result = _renderHook1.result;
      expect(result.current.initials).toBe('JD');
    });
  });
  describe('getInitials Function - Direct Testing', function () {
    it('should return "?" for non-string input', function () {
      var _require = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require.getInitials;
      expect(getInitials(null)).toBe('?');
      expect(getInitials(undefined)).toBe('?');
      expect(getInitials(123)).toBe('?');
      expect(getInitials({})).toBe('?');
    });
    it('should return "?" for empty string', function () {
      var _require2 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require2.getInitials;
      expect(getInitials('')).toBe('?');
    });
    it('should return "?" for whitespace-only string', function () {
      var _require3 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require3.getInitials;
      expect(getInitials('   ')).toBe('?');
    });
    it('should return first two characters for single word', function () {
      var _require4 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require4.getInitials;
      expect(getInitials('John')).toBe('JO');
      expect(getInitials('A')).toBe('A');
    });
    it('should return first and last initial for two words', function () {
      var _require5 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require5.getInitials;
      expect(getInitials('John Doe')).toBe('JD');
    });
    it('should return first and last initial for multiple words', function () {
      var _require6 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require6.getInitials;
      expect(getInitials('John Michael Doe')).toBe('JD');
      expect(getInitials('Mary Jane Watson Parker')).toBe('MP');
    });
    it('should handle names with multiple spaces', function () {
      var _require7 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require7.getInitials;
      expect(getInitials('John    Doe')).toBe('JD');
    });
    it('should return uppercase initials', function () {
      var _require8 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require8.getInitials;
      expect(getInitials('john doe')).toBe('JD');
      expect(getInitials('JOHN DOE')).toBe('JD');
    });
    it('should handle edge case with empty first or last name', function () {
      var _require9 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require9.getInitials;
      expect(getInitials(' John')).toBe('JO');
    });
    it('should return "?" when combined initials are empty', function () {
      var _require0 = require("../../../platform/components/display/Avatar/useAvatar"),
        getInitials = _require0.getInitials;
      expect(getInitials('   ')).toBe('?');
    });
  });
  describe('Edge Cases - Missing Props', function () {
    it('should handle image without testID', function () {
