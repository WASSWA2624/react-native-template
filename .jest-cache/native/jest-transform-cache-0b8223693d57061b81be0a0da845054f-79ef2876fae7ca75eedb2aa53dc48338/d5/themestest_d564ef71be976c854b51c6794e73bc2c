var getCjsOrEsmDefault = function getCjsOrEsmDefault(mod) {
  var _mod$default;
  return (_mod$default = mod == null ? void 0 : mod.default) != null ? _mod$default : mod;
};
var themeKeys = function themeKeys(theme) {
  return Object.keys(theme).sort();
};
describe('themes', function () {
  it('light and dark themes should have identical shape', function () {
    var lightTheme = getCjsOrEsmDefault(require("../../theme/light.theme"));
    var darkTheme = getCjsOrEsmDefault(require("../../theme/dark.theme"));
    expect(themeKeys(lightTheme)).toEqual(themeKeys(darkTheme));
  });
  it('themes should expose breakpoints and animations (used by UI)', function () {
    var lightTheme = getCjsOrEsmDefault(require("../../theme/light.theme"));
    expect(lightTheme.breakpoints).toEqual(expect.objectContaining({
      mobile: expect.any(Number),
      tablet: expect.any(Number),
      desktop: expect.any(Number),
      large: expect.any(Number)
    }));
    expect(lightTheme.animations).toEqual(expect.objectContaining({
      duration: expect.any(Object),
      easing: expect.any(Object)
    }));
  });
  it('theme index should expose both named exports and the dev-plan default export API', function () {
    var themeIndex = require("../../theme");
    var api = getCjsOrEsmDefault(themeIndex);
    var lightTheme = getCjsOrEsmDefault(require("../../theme/light.theme"));
    expect(api).toEqual(expect.objectContaining({
      ThemeProvider: expect.any(Function),
      lightTheme: expect.any(Object),
      darkTheme: expect.any(Object),
      getTheme: expect.any(Function)
    }));
    expect(themeIndex.ThemeProvider).toEqual(expect.any(Function));
    expect(themeIndex.getTheme).toEqual(expect.any(Function));
    expect(api.getTheme('light')).toEqual(lightTheme);
  });
});
