function getAccessibilityRole(variant, accessibilityRole) {
  if (typeof accessibilityRole === 'string' && accessibilityRole.length > 0) {
    return accessibilityRole;
  }
  if (variant === 'h1' || variant === 'h2' || variant === 'h3') {
