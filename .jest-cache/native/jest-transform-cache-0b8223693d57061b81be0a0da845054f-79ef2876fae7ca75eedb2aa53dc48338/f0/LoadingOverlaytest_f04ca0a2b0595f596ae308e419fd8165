var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _styledComponents = require("styled-components");
var _native = require("styled-components/native");
var _LoadingOverlay = _interopRequireDefault(require("../../../platform/components/feedback/LoadingOverlay/LoadingOverlay.web"));
var _LoadingOverlay2 = _interopRequireDefault(require("../../../platform/components/feedback/LoadingOverlay/LoadingOverlay.android"));
