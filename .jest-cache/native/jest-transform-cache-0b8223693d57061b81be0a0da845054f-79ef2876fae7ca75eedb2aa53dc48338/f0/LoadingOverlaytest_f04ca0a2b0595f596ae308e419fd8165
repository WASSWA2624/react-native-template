describe('LoadingOverlay Component', function () {
  it('does not render on web when hidden', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_styledComponents.ThemeProvider, {
        theme: _light.default,
        children: (0, _jsxRuntime.jsx)(_LoadingOverlay.default, {
          visible: false
        })
      })),
      queryByLabelText = _render.queryByLabelText;
    expect(queryByLabelText('common.loading')).toBeNull();
  });
  it('renders Android overlay with default title', function () {
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_native.ThemeProvider, {
        theme: _light.default,
        children: (0, _jsxRuntime.jsx)(_LoadingOverlay2.default, {
          visible: true
        })
      })),
      getByText = _render2.getByText;
    expect(getByText('shell.loadingOverlay.title')).toBeTruthy();
  });
  it('renders iOS overlay with custom message', function () {
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_native.ThemeProvider, {
        theme: _light.default,
        children: (0, _jsxRuntime.jsx)(_LoadingOverlay3.default, {
          visible: true,
          message: "Loading data"
        })
      })),
      getByText = _render3.getByText;
    expect(getByText('Loading data')).toBeTruthy();
  });
});
