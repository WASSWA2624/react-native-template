var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _react = _interopRequireDefault(require("react"));
var _reactTestRenderer = _interopRequireDefault(require("react-test-renderer"));
var _reactRedux = require("react-redux");
var _toolkit = require("@reduxjs/toolkit");
var _rootReducer = _interopRequireDefault(require("../../../../store/rootReducer"));
var _useLanguageControls = _interopRequireDefault(require("../../../../platform/components/navigation/LanguageControls/useLanguageControls"));
var _storage = require("../../../../services/storage");
var _jsxRuntime = require("react/jsx-runtime");
function _getJestObj() {
  var _require = require("@jest/globals"),
    jest = _require.jest;
  _getJestObj = function _getJestObj() {
    return jest;
  };
  return jest;
}
var act = _reactTestRenderer.default.act;
var createStore = function createStore(preloadedState) {
  return (0, _toolkit.configureStore)({
    reducer: _rootReducer.default,
    preloadedState: preloadedState
  });
};
var renderHook = function renderHook(hook) {
  var _ref = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : {},
    initialState = _ref.initialState;
  var result = {};
  var store = createStore(initialState);
  var HookHarness = function HookHarness() {
    var hookResult = hook();
    Object.assign(result, hookResult);
    return null;
  };
  var renderer;
  act(function () {
    renderer = _reactTestRenderer.default.create((0, _jsxRuntime.jsx)(_reactRedux.Provider, {
      store: store,
      children: (0, _jsxRuntime.jsx)(HookHarness, {})
    }));
  });
  return {
    result: {
      current: result
    },
    store: store,
    rerender: function rerender() {
      act(function () {
        renderer.update((0, _jsxRuntime.jsx)(_reactRedux.Provider, {
          store: store,
          children: (0, _jsxRuntime.jsx)(HookHarness, {})
        }));
      });
    }
  };
};
describe('useLanguageControls', function () {
  beforeEach(function () {
    jest.clearAllMocks();
  });
  it('returns locale and supported options', function () {
    var _renderHook = renderHook(_useLanguageControls.default, {
        initialState: {
          ui: {
            theme: 'light',
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook.result;
    expect(result.current.locale).toBe('en');
    expect(result.current.options).toHaveLength(2);
    expect(result.current.options[0].value).toBe('en');
  });
  it('falls back to default when stored locale is unsupported', function () {
    var _renderHook2 = renderHook(_useLanguageControls.default, {
        initialState: {
          ui: {
            theme: 'light',
            locale: 'xx',
            isLoading: false
          }
        }
      }),
      result = _renderHook2.result;
    expect(result.current.locale).toBe('en');
  });
  it('persists and dispatches when setting a valid locale', function () {
    var _renderHook3 = renderHook(_useLanguageControls.default, {
        initialState: {
          ui: {
            theme: 'light',
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook3.result,
      store = _renderHook3.store;
    act(function () {
      result.current.setLocale('fr');
    });
    expect(store.getState().ui.locale).toBe('fr');
    expect(_storage.async.setItem).toHaveBeenCalledWith('user_locale', 'fr');
  });
  it('ignores invalid locale values', function () {
    var _renderHook4 = renderHook(_useLanguageControls.default, {
        initialState: {
          ui: {
            theme: 'light',
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook4.result,
      store = _renderHook4.store;
    act(function () {
      result.current.setLocale('invalid');
    });
    expect(store.getState().ui.locale).toBe('en');
    expect(_storage.async.setItem).not.toHaveBeenCalled();
  });
  it('avoids persisting when locale is unchanged', function () {
    var _renderHook5 = renderHook(_useLanguageControls.default, {
        initialState: {
          ui: {
            theme: 'light',
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook5.result,
      store = _renderHook5.store;
    act(function () {
      result.current.setLocale('en');
    });
    expect(store.getState().ui.locale).toBe('en');
    expect(_storage.async.setItem).not.toHaveBeenCalled();
  });
});
