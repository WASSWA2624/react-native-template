var getCjsOrEsmDefault = function getCjsOrEsmDefault(mod) {
  var _mod$default;
  return (_mod$default = mod == null ? void 0 : mod.default) != null ? _mod$default : mod;
};
describe('Shadow Tokens (Step 3.5)', function () {
  it('should export all shadow tokens', function () {
    var shadows = getCjsOrEsmDefault(require("../../../theme/tokens/shadows"));
    expect(shadows).toEqual(expect.any(Object));
  });
  it('should have correct shadow token structure', function () {
    var shadows = getCjsOrEsmDefault(require("../../../theme/tokens/shadows"));
    expect(shadows).toEqual(expect.objectContaining({
      sm: expect.any(Object),
      md: expect.any(Object)
    }));
    expect(shadows.sm).toEqual(expect.objectContaining({
      shadowColor: expect.any(String),
      shadowOffset: expect.objectContaining({
        width: expect.any(Number),
        height: expect.any(Number)
      }),
      shadowOpacity: expect.any(Number),
      shadowRadius: expect.any(Number),
      elevation: expect.any(Number)
    }));
    expect(shadows.md).toEqual(expect.objectContaining({
      shadowColor: expect.any(String),
      shadowOffset: expect.objectContaining({
        width: expect.any(Number),
        height: expect.any(Number)
      }),
      shadowOpacity: expect.any(Number),
      shadowRadius: expect.any(Number),
      elevation: expect.any(Number)
    }));
  });
});
