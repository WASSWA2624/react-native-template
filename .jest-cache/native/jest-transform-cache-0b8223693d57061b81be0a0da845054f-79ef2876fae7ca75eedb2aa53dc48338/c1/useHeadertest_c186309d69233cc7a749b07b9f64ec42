var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _reactNative = require("@testing-library/react-native");
var _useHeader = _interopRequireDefault(require("../../../../platform/components/navigation/Header/useHeader"));
function _getJestObj() {
  var _require = require("@jest/globals"),
    jest = _require.jest;
  _getJestObj = function _getJestObj() {
    return jest;
  };
  return jest;
}
describe('useHeader', function () {
  it('returns resolvedLabel from accessibilityLabel when provided', function () {
    var _renderHook = (0, _reactNative.renderHook)(function () {
        return (0, _useHeader.default)({
          accessibilityLabel: 'Custom label',
          title: 'Title'
        });
      }),
      result = _renderHook.result;
    expect(result.current.resolvedLabel).toBe('Custom label');
  });
  it('returns title as resolvedLabel when string title and no accessibilityLabel', function () {
    var _renderHook2 = (0, _reactNative.renderHook)(function () {
        return (0, _useHeader.default)({
          title: 'My Page'
        });
      }),
      result = _renderHook2.result;
    expect(result.current.resolvedLabel).toBe('My Page');
  });
  it('returns default i18n key when no title and no accessibilityLabel', function () {
    var _renderHook3 = (0, _reactNative.renderHook)(function () {
        return (0, _useHeader.default)({});
      }),
      result = _renderHook3.result;
    expect(result.current.resolvedLabel).toBe('navigation.header.title');
  });
  it('returns empty string when title is empty string and no accessibilityLabel', function () {
    var _renderHook4 = (0, _reactNative.renderHook)(function () {
        return (0, _useHeader.default)({
          title: ''
        });
      }),
      result = _renderHook4.result;
    expect(result.current.resolvedLabel).toBe('');
  });
  it('handles undefined options', function () {
    var _renderHook5 = (0, _reactNative.renderHook)(function () {
        return (0, _useHeader.default)();
      }),
      result = _renderHook5.result;
    expect(result.current).toHaveProperty('resolvedLabel');
  });
});
