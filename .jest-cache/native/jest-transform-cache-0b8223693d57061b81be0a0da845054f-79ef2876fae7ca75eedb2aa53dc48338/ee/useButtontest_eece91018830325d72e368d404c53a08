1616f9e83e2aaf57c55a28fcba961585
var _jsxRuntime = require("react/jsx-runtime");
var React = require('react');
var _require = require('react-test-renderer'),
  act = _require.act,
  create = _require.create;
var useButtonModule = require("../../../../platform/components/forms/Button/useButton");
var useButton = useButtonModule.default || useButtonModule;
var typesModule = require("../../../../platform/components/forms/Button/types");
var _ref = typesModule.default || typesModule,
  STATES = _ref.STATES;
var renderHook = function renderHook(hook, hookProps) {
  var result = {
    current: null
  };
  var HookHarness = function HookHarness(props) {
    result.current = hook(props.hookProps);
    return null;
  };
  var renderer;
  act(function () {
    renderer = create((0, _jsxRuntime.jsx)(HookHarness, {
      hookProps: hookProps
    }));
  });
  return {
    result: result,
    rerender: function rerender(nextProps) {
      return act(function () {
        renderer.update((0, _jsxRuntime.jsx)(HookHarness, {
          hookProps: nextProps
        }));
      });
    },
    unmount: function unmount() {
      return act(function () {
        renderer.unmount();
      });
    }
  };
};
describe('useButton', function () {
  describe('Initialization', function () {
    it('should initialize with default state', function () {
      var _renderHook = renderHook(useButton, {}),
        result = _renderHook.result;
      expect(result.current.isPressed).toBe(false);
      expect(result.current.state).toBe(STATES.DEFAULT);
      expect(typeof result.current.handlePress).toBe('function');
      expect(typeof result.current.handlePressIn).toBe('function');
      expect(typeof result.current.handlePressOut).toBe('function');
    });
    it('should initialize with disabled state when disabled prop is true', function () {
      var _renderHook2 = renderHook(useButton, {
          disabled: true
        }),
        result = _renderHook2.result;
      expect(result.current.state).toBe(STATES.DISABLED);
    });
    it('should initialize with loading state when loading prop is true', function () {
      var _renderHook3 = renderHook(useButton, {
          loading: true
        }),
        result = _renderHook3.result;
      expect(result.current.state).toBe(STATES.LOADING);
    });
  });
  describe('Press Handlers', function () {
    it('should call onPress when handlePress is called', function () {
      var mockOnPress = jest.fn();
      var _renderHook4 = renderHook(useButton, {
          onPress: mockOnPress
        }),
        result = _renderHook4.result;
      act(function () {
        result.current.handlePress();
      });
      expect(mockOnPress).toHaveBeenCalledTimes(1);
    });
    it('should not call onPress when disabled', function () {
      var mockOnPress = jest.fn();
      var _renderHook5 = renderHook(useButton, {
          onPress: mockOnPress,
          disabled: true
        }),
        result = _renderHook5.result;
      act(function () {
        result.current.handlePress();
      });
      expect(mockOnPress).not.toHaveBeenCalled();
    });
    it('should not call onPress when loading', function () {
      var mockOnPress = jest.fn();
      var _renderHook6 = renderHook(useButton, {
          onPress: mockOnPress,
          loading: true
        }),
        result = _renderHook6.result;
      act(function () {
        result.current.handlePress();
      });
      expect(mockOnPress).not.toHaveBeenCalled();
    });
    it('should pass event to onPress', function () {
      var mockOnPress = jest.fn();
      var mockEvent = {
        type: 'press'
      };
      var _renderHook7 = renderHook(useButton, {
          onPress: mockOnPress
        }),
        result = _renderHook7.result;
      act(function () {
        result.current.handlePress(mockEvent);
      });
      expect(mockOnPress).toHaveBeenCalledWith(mockEvent);
    });
  });
  describe('Press In/Out Handlers', function () {
    it('should set isPressed to true when handlePressIn is called', function () {
      var _renderHook8 = renderHook(useButton, {}),
        result = _renderHook8.result;
      act(function () {
        result.current.handlePressIn();
      });
      expect(result.current.isPressed).toBe(true);
      expect(result.current.state).toBe(STATES.ACTIVE);
    });
    it('should not set isPressed when disabled', function () {
      var _renderHook9 = renderHook(useButton, {
          disabled: true
        }),
        result = _renderHook9.result;
      act(function () {
        result.current.handlePressIn();
      });
      expect(result.current.isPressed).toBe(false);
      expect(result.current.state).toBe(STATES.DISABLED);
    });
    it('should not set isPressed when loading', function () {
      var _renderHook0 = renderHook(useButton, {
          loading: true
        }),
        result = _renderHook0.result;
      act(function () {
        result.current.handlePressIn();
      });
      expect(result.current.isPressed).toBe(false);
      expect(result.current.state).toBe(STATES.LOADING);
    });
    it('should set isPressed to false when handlePressOut is called', function () {
      var _renderHook1 = renderHook(useButton, {}),
        result = _renderHook1.result;
      act(function () {
        result.current.handlePressIn();
      });
      expect(result.current.isPressed).toBe(true);
      act(function () {
        result.current.handlePressOut();
      });
      expect(result.current.isPressed).toBe(false);
      expect(result.current.state).toBe(STATES.DEFAULT);
    });
  });
  describe('State Management', function () {
    it('should return LOADING state when loading is true', function () {
      var _renderHook10 = renderHook(useButton, {
          loading: true
        }),
        result = _renderHook10.result;
      expect(result.current.state).toBe(STATES.LOADING);
    });
    it('should return DISABLED state when disabled is true', function () {
      var _renderHook11 = renderHook(useButton, {
          disabled: true
        }),
        result = _renderHook11.result;
      expect(result.current.state).toBe(STATES.DISABLED);
    });
    it('should return ACTIVE state when isPressed is true', function () {
      var _renderHook12 = renderHook(useButton, {}),
        result = _renderHook12.result;
      act(function () {
        result.current.handlePressIn();
      });
      expect(result.current.state).toBe(STATES.ACTIVE);
    });
    it('should return DEFAULT state when not loading, disabled, or pressed', function () {
      var _renderHook13 = renderHook(useButton, {}),
        result = _renderHook13.result;
      expect(result.current.state).toBe(STATES.DEFAULT);
    });
    it('should prioritize loading over disabled', function () {
      var _renderHook14 = renderHook(useButton, {
          loading: true,
          disabled: true
        }),
        result = _renderHook14.result;
      expect(result.current.state).toBe(STATES.LOADING);
    });
    it('should prioritize loading over pressed', function () {
      var _renderHook15 = renderHook(useButton, {
          loading: true
        }),
        result = _renderHook15.result;
      act(function () {
        result.current.handlePressIn();
      });
      expect(result.current.state).toBe(STATES.LOADING);
    });
    it('should prioritize disabled over pressed', function () {
      var _renderHook16 = renderHook(useButton, {
          disabled: true
        }),
        result = _renderHook16.result;
      act(function () {
        result.current.handlePressIn();
      });
      expect(result.current.state).toBe(STATES.DISABLED);
    });
  });
  describe('Edge Cases', function () {
    it('should handle undefined onPress', function () {
      var _renderHook17 = renderHook(useButton, {
          onPress: undefined
        }),
        result = _renderHook17.result;
      act(function () {
        result.current.handlePress();
      });
      expect(result.current.state).toBe(STATES.DEFAULT);
    });
    it('should handle null onPress', function () {
      var _renderHook18 = renderHook(useButton, {
          onPress: null
        }),
        result = _renderHook18.result;
      act(function () {
        result.current.handlePress();
      });
      expect(result.current.state).toBe(STATES.DEFAULT);
    });
    it('should handle multiple rapid press in/out', function () {
      var _renderHook19 = renderHook(useButton, {}),
        result = _renderHook19.result;
      act(function () {
        result.current.handlePressIn();
        result.current.handlePressOut();
        result.current.handlePressIn();
        result.current.handlePressOut();
      });
      expect(result.current.isPressed).toBe(false);
      expect(result.current.state).toBe(STATES.DEFAULT);
    });
  });
});
