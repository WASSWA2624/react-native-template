describe('SystemBanner Component', function () {
  it('renders web banner when visible', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_styledComponents.ThemeProvider, {
        theme: _light.default,
        children: (0, _jsxRuntime.jsx)(_SystemBanner.default, {
          visible: true,
          title: "Status",
          message: "All good"
        })
      })),
      getByLabelText = _render.getByLabelText;
    expect(getByLabelText('Status All good')).toBeTruthy();
  });
  it('does not render when invisible', function () {
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_styledComponents.ThemeProvider, {
        theme: _light.default,
        children: (0, _jsxRuntime.jsx)(_SystemBanner.default, {
          visible: false,
          title: "Hidden",
          message: "Nope"
        })
      })),
      queryByText = _render2.queryByText;
    expect(queryByText('Hidden')).toBeNull();
  });
  it('fires action on Android', function () {
    var onAction = jest.fn();
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_native.ThemeProvider, {
        theme: _light.default,
        children: (0, _jsxRuntime.jsx)(_SystemBanner2.default, {
          variant: _SystemBanner4.VARIANTS.OFFLINE,
          title: "Offline",
          message: "Reconnect",
          actionLabel: "Retry",
          onAction: onAction
        })
      })),
      getByLabelText = _render3.getByLabelText;
    _reactNative.fireEvent.press(getByLabelText('Retry'));
    expect(onAction).toHaveBeenCalled();
  });
  it('fires dismiss on iOS', function () {
    var onDismiss = jest.fn();
    var _render4 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_native.ThemeProvider, {
        theme: _light.default,
        children: (0, _jsxRuntime.jsx)(_SystemBanner3.default, {
          variant: _SystemBanner4.VARIANTS.MAINTENANCE,
          title: "Maintenance",
          message: "Please wait",
          dismissLabel: "Close",
          onDismiss: onDismiss
        })
      })),
      getByLabelText = _render4.getByLabelText;
    _reactNative.fireEvent.press(getByLabelText('Close'));
    expect(onDismiss).toHaveBeenCalled();
  });
});
