var GlobalHeader = _GlobalHeader.default.default || _GlobalHeader.default;
describe('GlobalHeader Component', function () {
  var actions = [{
    key: 'export',
    label: 'Export',
    onPress: jest.fn(),
    roles: ['admin'],
    variant: _GlobalHeader.ACTION_VARIANTS.PRIMARY
  }, {
    key: 'refresh',
    label: 'Refresh',
    onPress: jest.fn(),
    roles: ['user']
  }, {
    key: 'filter',
    label: 'Filter',
    onPress: jest.fn(),
    placement: _GlobalHeader.ACTION_PLACEMENTS.SECONDARY,
    icon: (0, _jsxRuntime.jsx)(_reactNative.Text, {
      children: "+"
    })
  }];
  it('should render title and subtitle', function () {
    var _renderWithProviders = (0, _testUtils.renderWithProviders)((0, _jsxRuntime.jsx)(GlobalHeader, {
        title: "Dashboard",
        subtitle: "Today",
        testID: "global-header"
      })),
      getByText = _renderWithProviders.getByText;
    expect(getByText('Dashboard')).toBeTruthy();
    expect(getByText('Today')).toBeTruthy();
  });
  it('should render custom title node', function () {
    var _renderWithProviders2 = (0, _testUtils.renderWithProviders)((0, _jsxRuntime.jsx)(GlobalHeader, {
        title: (0, _jsxRuntime.jsx)(_reactNative.Text, {
          children: "Custom Title"
        }),
        testID: "global-header"
      })),
      getByText = _renderWithProviders2.getByText;
    expect(getByText('Custom Title')).toBeTruthy();
  });
  it('should filter actions by current role', function () {
    var _renderWithProviders3 = (0, _testUtils.renderWithProviders)((0, _jsxRuntime.jsx)(GlobalHeader, {
        actions: actions,
        currentRole: "admin",
        testID: "global-header"
      })),
      getByTestId = _renderWithProviders3.getByTestId,
      queryByTestId = _renderWithProviders3.queryByTestId;
    expect(getByTestId('global-header-action-export')).toBeTruthy();
    expect(getByTestId('global-header-action-filter')).toBeTruthy();
    expect(queryByTestId('global-header-action-refresh')).toBeNull();
  });
  it('should render action without label safely', function () {
    var labelLessActions = [{
      key: 'icon-only',
      onPress: jest.fn(),
