var getCjsOrEsmDefault = function getCjsOrEsmDefault(mod) {
  var _mod$default;
  return (_mod$default = mod == null ? void 0 : mod.default) != null ? _mod$default : mod;
};
describe('Dark Theme (Step 3.7)', function () {
  it('should have correct dark theme structure', function () {
    var darkTheme = getCjsOrEsmDefault(require("../../theme/dark.theme"));
    expect(darkTheme).toEqual(expect.objectContaining({
      colors: expect.any(Object),
      spacing: expect.any(Object),
      typography: expect.any(Object),
      radius: expect.any(Object),
      shadows: expect.any(Object),
      mode: 'dark'
    }));
  });
  it('should override colors for dark mode', function () {
    var darkTheme = getCjsOrEsmDefault(require("../../theme/dark.theme"));
    var lightTheme = getCjsOrEsmDefault(require("../../theme/light.theme"));
    expect(darkTheme.colors.background.primary).not.toBe(lightTheme.colors.background.primary);
    expect(darkTheme.colors.text.primary).not.toBe(lightTheme.colors.text.primary);
  });
  it('should import all token types', function () {
    var darkTheme = getCjsOrEsmDefault(require("../../theme/dark.theme"));
    expect(darkTheme.colors).toBeDefined();
    expect(darkTheme.spacing).toBeDefined();
    expect(darkTheme.typography).toBeDefined();
    expect(darkTheme.radius).toBeDefined();
    expect(darkTheme.shadows).toBeDefined();
    expect(darkTheme.breakpoints).toBeDefined();
    expect(darkTheme.animations).toBeDefined();
  });
});
