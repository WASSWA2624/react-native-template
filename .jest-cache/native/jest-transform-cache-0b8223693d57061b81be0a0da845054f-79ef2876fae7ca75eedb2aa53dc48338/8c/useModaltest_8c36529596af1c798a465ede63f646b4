var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _react = _interopRequireDefault(require("react"));
var _reactTestRenderer = _interopRequireDefault(require("react-test-renderer"));
var _useModal = _interopRequireDefault(require("../../../../platform/components/feedback/Modal/useModal"));
var _jsxRuntime = require("react/jsx-runtime");
var act = _reactTestRenderer.default.act;
var renderHook = function renderHook(hook) {
  var _ref = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : {},
    initialProps = _ref.initialProps;
  var result = {};
  var renderer;
  var HookHarness = function HookHarness(_ref2) {
    var hookProps = _ref2.hookProps;
    var hookResult = hook(hookProps);
    Object.assign(result, hookResult);
    return null;
  };
  act(function () {
    renderer = _reactTestRenderer.default.create((0, _jsxRuntime.jsx)(HookHarness, {
      hookProps: initialProps
    }));
  });
  return {
    result: {
      current: result
    },
    rerender: function rerender(newProps) {
      act(function () {
        renderer.update((0, _jsxRuntime.jsx)(HookHarness, {
          hookProps: newProps
        }));
      });
    },
    unmount: function unmount() {
      act(function () {
        renderer.unmount();
      });
    }
  };
};
describe('useModal', function () {
  beforeEach(function () {
    jest.clearAllMocks();
  });
  describe('Initialization', function () {
    it('should return handleBackdropPress function', function () {
      var _renderHook = renderHook(_useModal.default, {
          initialProps: {
            visible: true,
            onDismiss: jest.fn()
          }
        }),
        result = _renderHook.result;
      expect(typeof result.current.handleBackdropPress).toBe('function');
    });
  });
  describe('Backdrop Press Handler', function () {
    it('should call onDismiss when handleBackdropPress is called and dismissOnBackdrop is true', function () {
      var mockOnDismiss = jest.fn();
      var _renderHook2 = renderHook(_useModal.default, {
          initialProps: {
            visible: true,
            onDismiss: mockOnDismiss,
            dismissOnBackdrop: true
          }
        }),
        result = _renderHook2.result;
      act(function () {
        result.current.handleBackdropPress();
      });
      expect(mockOnDismiss).toHaveBeenCalledTimes(1);
    });
    it('should not call onDismiss when dismissOnBackdrop is false', function () {
      var mockOnDismiss = jest.fn();
      var _renderHook3 = renderHook(_useModal.default, {
          initialProps: {
            visible: true,
            onDismiss: mockOnDismiss,
            dismissOnBackdrop: false
          }
        }),
        result = _renderHook3.result;
      act(function () {
        result.current.handleBackdropPress();
      });
      expect(mockOnDismiss).not.toHaveBeenCalled();
    });
    it('should not call onDismiss when onDismiss is not provided', function () {
      var _renderHook4 = renderHook(_useModal.default, {
          initialProps: {
            visible: true,
            dismissOnBackdrop: true
          }
        }),
        result = _renderHook4.result;
      act(function () {
        result.current.handleBackdropPress();
      });
      expect(result.current.handleBackdropPress).toBeDefined();
    });
    it('should default dismissOnBackdrop to true', function () {
      var mockOnDismiss = jest.fn();
      var _renderHook5 = renderHook(_useModal.default, {
          initialProps: {
            visible: true,
            onDismiss: mockOnDismiss
          }
        }),
        result = _renderHook5.result;
      act(function () {
        result.current.handleBackdropPress();
      });
      expect(mockOnDismiss).toHaveBeenCalledTimes(1);
    });
  });
  describe('Edge Cases', function () {
    it('should handle undefined onDismiss', function () {
      var _renderHook6 = renderHook(_useModal.default, {
          initialProps: {
            visible: true,
            onDismiss: undefined
          }
        }),
        result = _renderHook6.result;
      act(function () {
        result.current.handleBackdropPress();
      });
      expect(result.current.handleBackdropPress).toBeDefined();
    });
    it('should handle null onDismiss', function () {
      var _renderHook7 = renderHook(_useModal.default, {
          initialProps: {
            visible: true,
            onDismiss: null
          }
        }),
        result = _renderHook7.result;
      act(function () {
        result.current.handleBackdropPress();
      });
      expect(result.current.handleBackdropPress).toBeDefined();
    });
    it('should handle multiple backdrop presses', function () {
      var mockOnDismiss = jest.fn();
      var _renderHook8 = renderHook(_useModal.default, {
          initialProps: {
            visible: true,
            onDismiss: mockOnDismiss
          }
        }),
        result = _renderHook8.result;
      act(function () {
        result.current.handleBackdropPress();
        result.current.handleBackdropPress();
        result.current.handleBackdropPress();
      });
      expect(mockOnDismiss).toHaveBeenCalledTimes(3);
    });
  });
});
