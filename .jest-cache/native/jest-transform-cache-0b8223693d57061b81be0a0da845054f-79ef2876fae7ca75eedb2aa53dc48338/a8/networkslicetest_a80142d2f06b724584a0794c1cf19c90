var _network = require("../../store/slices/network.slice");
var _networkQuality = require("../../utils/networkQuality");
describe('Network Slice', function () {
  var initialState = {
    isOnline: true,
    isSyncing: false,
    quality: _networkQuality.NETWORK_QUALITY.UNKNOWN
  };
  beforeEach(function () {
    jest.clearAllMocks();
  });
  describe('initialState', function () {
    it('should have correct initial state', function () {
      var state = (0, _network.reducer)(undefined, {
        type: 'unknown'
      });
      expect(state).toEqual(initialState);
    });
  });
  describe('setOnline', function () {
    it('should set online status to true', function () {
      var state = (0, _network.reducer)(initialState, _network.actions.setOnline(true));
      expect(state.isOnline).toBe(true);
    });
    it('should set online status to false', function () {
      var state = (0, _network.reducer)(initialState, _network.actions.setOnline(false));
      expect(state.isOnline).toBe(false);
    });
    it('should toggle online status', function () {
      var offlineState = (0, _network.reducer)(initialState, _network.actions.setOnline(false));
      var onlineState = (0, _network.reducer)(offlineState, _network.actions.setOnline(true));
      expect(onlineState.isOnline).toBe(true);
    });
  });
  describe('setSyncing', function () {
    it('should set syncing status to true', function () {
      var state = (0, _network.reducer)(initialState, _network.actions.setSyncing(true));
      expect(state.isSyncing).toBe(true);
    });
    it('should set syncing status to false', function () {
      var syncingState = (0, _network.reducer)(initialState, _network.actions.setSyncing(true));
      var notSyncingState = (0, _network.reducer)(syncingState, _network.actions.setSyncing(false));
      expect(notSyncingState.isSyncing).toBe(false);
    });
  });
  describe('setQuality', function () {
    it('should set network quality', function () {
      var state = (0, _network.reducer)(initialState, _network.actions.setQuality(_networkQuality.NETWORK_QUALITY.LOW));
      expect(state.quality).toBe(_networkQuality.NETWORK_QUALITY.LOW);
    });
  });
  describe('multiple actions', function () {
    it('should handle multiple state updates', function () {
      var state = (0, _network.reducer)(initialState, _network.actions.setOnline(false));
      state = (0, _network.reducer)(state, _network.actions.setSyncing(true));
      expect(state.isOnline).toBe(false);
      expect(state.isSyncing).toBe(true);
    });
  });
});
