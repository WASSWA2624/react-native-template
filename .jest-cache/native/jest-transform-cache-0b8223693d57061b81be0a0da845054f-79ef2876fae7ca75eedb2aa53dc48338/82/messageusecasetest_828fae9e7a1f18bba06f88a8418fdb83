_getJestObj().mock("../../../features/message/message.api", function () {
  return {
    messageApi: {
      list: jest.fn(),
      get: jest.fn(),
      create: jest.fn(),
      update: jest.fn(),
      remove: jest.fn()
    },
    getMessagesByConversationApi: jest.fn(),
    getMessageMediaApi: jest.fn()
  };
});
_getJestObj().mock("../../../offline/request", function () {
  return {
    queueRequestIfOffline: jest.fn()
  };
});
var _message = require("../../../features/message");
var _message2 = require("../../../features/message/message.api");
var _request = require("../../../offline/request");
var _crudUsecaseRunner = require("../../helpers/crud-usecase-runner");
function _getJestObj() {
  var _require = require("@jest/globals"),
    jest = _require.jest;
  _getJestObj = function _getJestObj() {
    return jest;
  };
  return jest;
}
describe('message.usecase', function () {
  beforeEach(function () {
    _message2.messageApi.list.mockResolvedValue({
      data: [{
        id: '1'
      }]
    });
    _message2.messageApi.get.mockResolvedValue({
      data: {
        id: '1'
      }
    });
    _message2.messageApi.create.mockResolvedValue({
      data: {
        id: '1'
      }
    });
    _message2.messageApi.update.mockResolvedValue({
      data: {
        id: '1'
      }
    });
    _message2.messageApi.remove.mockResolvedValue({
      data: {
        id: '1'
      }
    });
    _message2.getMessagesByConversationApi.mockResolvedValue({
      data: [{
        id: '1'
      }]
    });
    _message2.getMessageMediaApi.mockResolvedValue({
      data: {
        id: '1'
      }
    });
  });
  (0, _crudUsecaseRunner.runCrudUsecaseTests)({
    list: _message.listMessages,
    get: _message.getMessage,
    create: _message.createMessage,
    update: _message.updateMessage,
    remove: _message.deleteMessage,
    extraActions: [{
      fn: _message.listMessagesByConversation,
      args: ['1']
    }, {
      fn: _message.getMessageMedia,
      args: ['1']
    }]
  }, {
    queueRequestIfOffline: _request.queueRequestIfOffline
  });
});
