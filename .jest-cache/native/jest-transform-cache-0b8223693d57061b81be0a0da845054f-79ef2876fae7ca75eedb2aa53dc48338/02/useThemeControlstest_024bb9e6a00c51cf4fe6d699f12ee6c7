        t: function t(key) {
          return key;
        },
        locale: 'en'
      };
    }
  };
});
_getJestObj().mock("../../../../services/storage", function () {
  return {
    async: {
      setItem: jest.fn()
    }
  };
});
var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _react = _interopRequireDefault(require("react"));
var _reactTestRenderer = _interopRequireDefault(require("react-test-renderer"));
var _reactRedux = require("react-redux");
var _toolkit = require("@reduxjs/toolkit");
var _rootReducer = _interopRequireDefault(require("../../../../store/rootReducer"));
var _useThemeControls = _interopRequireDefault(require("../../../../platform/components/navigation/ThemeControls/useThemeControls"));
var _types = require("../../../../platform/components/navigation/ThemeControls/types");
var _storage = require("../../../../services/storage");
var _jsxRuntime = require("react/jsx-runtime");
function _getJestObj() {
  var _require = require("@jest/globals"),
    jest = _require.jest;
  _getJestObj = function _getJestObj() {
    return jest;
  };
  return jest;
}
var act = _reactTestRenderer.default.act;
var createStore = function createStore(preloadedState) {
  return (0, _toolkit.configureStore)({
    reducer: _rootReducer.default,
    preloadedState: preloadedState
  });
};
var renderHook = function renderHook(hook) {
  var _ref = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : {},
    initialState = _ref.initialState;
  var result = {};
  var store = createStore(initialState);
  var HookHarness = function HookHarness() {
    var hookResult = hook();
    Object.assign(result, hookResult);
    return null;
  };
  var renderer;
  act(function () {
    renderer = _reactTestRenderer.default.create((0, _jsxRuntime.jsx)(_reactRedux.Provider, {
      store: store,
      children: (0, _jsxRuntime.jsx)(HookHarness, {})
    }));
  });
  return {
    result: {
      current: result
    },
    store: store,
    rerender: function rerender() {
      act(function () {
        renderer.update((0, _jsxRuntime.jsx)(_reactRedux.Provider, {
          store: store,
          children: (0, _jsxRuntime.jsx)(HookHarness, {})
        }));
      });
    }
  };
};
describe('useThemeControls', function () {
  beforeEach(function () {
    jest.clearAllMocks();
  });
  it('returns theme options and resolved theme', function () {
    var _renderHook = renderHook(_useThemeControls.default, {
        initialState: {
          ui: {
            theme: _types.THEME_MODES.DARK,
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook.result;
    expect(result.current.theme).toBe(_types.THEME_MODES.DARK);
    expect(result.current.options).toHaveLength(3);
    expect(result.current.options[0].value).toBe(_types.THEME_MODES.LIGHT);
  });
  it('falls back to light theme when state is invalid', function () {
    var _renderHook2 = renderHook(_useThemeControls.default, {
        initialState: {
          ui: {
            theme: 'invalid',
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook2.result;
    expect(result.current.theme).toBe(_types.THEME_MODES.LIGHT);
  });
  it('persists and dispatches when setting a valid theme', function () {
    var _renderHook3 = renderHook(_useThemeControls.default, {
        initialState: {
          ui: {
            theme: _types.THEME_MODES.LIGHT,
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook3.result,
      store = _renderHook3.store;
    act(function () {
      result.current.setTheme(_types.THEME_MODES.DARK);
    });
    expect(store.getState().ui.theme).toBe(_types.THEME_MODES.DARK);
    expect(_storage.async.setItem).toHaveBeenCalledWith('theme_preference', _types.THEME_MODES.DARK);
  });
  it('ignores invalid theme values', function () {
    var _renderHook4 = renderHook(_useThemeControls.default, {
        initialState: {
          ui: {
            theme: _types.THEME_MODES.LIGHT,
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook4.result,
      store = _renderHook4.store;
    act(function () {
      result.current.setTheme('invalid-value');
    });
    expect(store.getState().ui.theme).toBe(_types.THEME_MODES.LIGHT);
    expect(_storage.async.setItem).not.toHaveBeenCalled();
  });
  it('avoids persisting when theme is unchanged', function () {
    var _renderHook5 = renderHook(_useThemeControls.default, {
        initialState: {
          ui: {
            theme: _types.THEME_MODES.LIGHT,
            locale: 'en',
            isLoading: false
          }
        }
      }),
      result = _renderHook5.result,
      store = _renderHook5.store;
    act(function () {
      result.current.setTheme(_types.THEME_MODES.LIGHT);
    });
    expect(store.getState().ui.theme).toBe(_types.THEME_MODES.LIGHT);
    expect(_storage.async.setItem).not.toHaveBeenCalled();
  });
});
