{
  "name": "biomed-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
    "debug:expo": "node scripts/debug/expo-with-logging.mjs",
    "debug:android": "node scripts/debug/android-debug-logcat.mjs",
    "debug:ios": "node scripts/debug/ios-debug-logcat.mjs",
    "debug:web": "node scripts/debug/web-log-receiver.mjs",
    "debug:all": "node scripts/debug/debug-all.mjs"
  },
  "postinstall": "patch-package",
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@remix-run/router": "^1.23.0",
    "expo": "~54.0.32",
    "expo-constants": "~18.0.13",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "^17.0.8",
    "expo-localization": "^17.0.8",
    "expo-router": "~6.0.22",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.19",
    "zod": "^4.2.1"
  },
  "private": true,
  "overrides": {
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-native": "^13.3.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.2",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.2",
    "prettier": "^3.7.4",
    "react-test-renderer": "19.1.0",
    "workbox-webpack-plugin": "^7.4.0",
    "patch-package": "^8.0.0"
  }
